{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    }
    {
      "name": "Launch current file",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": [],
      "env": {},
      "buildFlags": ""
    },
    {
      "name": "Launch w3schools/debug package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/w3schools/debug",
      "args": [],
      "env": {},
      "buildFlags": ""
    },
    {
      "name": "Launch tests in current package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": [],
      "env": {},
      "buildFlags": ""
    },
    {
      "name": "Debug will_crash (Delve)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/w3schools/debug/will_crash",
      "args": [],
      "env": {},
      "buildFlags": "",
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "stopOnEntry": false
    },
    {
      "name": "Debug func_crash (Delve)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/w3schools/debug/func_crash",
      "args": [],
      "env": {},
      "buildFlags": "",
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "stopOnEntry": false
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}"
    }
    ,
    {
      "name": "Attach to Headless Delve",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1"
    }
  ]
}
